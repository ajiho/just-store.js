{
  "name": "just-store.js",
  "version": "1.0.0",
  "description": "Make the APIs of LocalStorage and SessionStorage easier",
  "repository": "github:ajiho/just-store.js",
  "bugs": {
    "url": "https://github.com/ajiho/just-store.js/issues"
  },
  "author": "ajiho",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky || true",
    "style": "prettier --config configs/prettier.config.mjs . --check --cache --cache-location node_modules/.cache/.prettiercache",
    "style:fix": "prettier --config configs/prettier.config.mjs . --write",
    "lint": "eslint --config configs/eslint.config.mjs \"src/**/*.ts\" --ext .ts --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:fix": "eslint --config configs/eslint.config.mjs \"src/**/*.ts\" --ext .ts --fix",
    "commitlint": "commitlint --config configs/commitlint.config.mjs -e -V",
    "sizelint": "size-limit --config configs/size-limit.mjs",
    "check": "run-s style lint",
    "fix": "run-s *:fix",
    "clean": "rimraf dist",
    "build:base": "rollup --config configs/rollup.config.mjs",
    "build:min": "cross-env NODE_ENV=production run-s build:base",
    "build": "run-s clean build:* sizelint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.mjs",
    "test": "vitest --config configs/vitest.config.mjs --coverage --ui",
    "coverage": "vitest --config configs/vitest.config.mjs run --coverage",
    "docs:dev": "vitepress dev docs --open",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "main": "dist/esm/index.mjs",
  "module": "dist/cjs/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "localStorage",
    "sessionStorage",
    "storage",
    "store"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "browser-sync": "^3.0.4",
    "conventional-changelog-cli": "^5.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.1",
    "husky": "^9.1.7",
    "is-what": "^5.4.0",
    "is-what-type": "^1.1.4",
    "jsdom": "^26.1.0",
    "medium-zoom": "^1.1.0",
    "npm-run-all2": "^8.0.4",
    "picocolors": "^1.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-sort-pkgjson": "^1.0.0",
    "prompts": "^2.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-dts": "^6.2.3",
    "sass": "^1.91.0",
    "semver": "^7.7.2",
    "size-limit": "^11.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vitepress": "^1.6.4",
    "vitepress-plugin-tabs": "^0.7.1",
    "vitest": "^3.2.4"
  }
}
